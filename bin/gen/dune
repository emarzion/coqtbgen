(coq.extraction
  (prelude ExtractRomanWheel)
  (extracted_modules TBGen)
  (theories Games))

(library
  (name extracted_code)
  (modules
    ;; Patch code
    M

    ;; Extracted code
    TBGen
  ))

(env
